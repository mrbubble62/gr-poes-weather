<?xml version="1.0"?>
<block>
  <name>FY-1 HRPT Decoder</name>
  <key>poesweather_fy1_hrpt_decoder</key>
  <category>POES Weather</category>
  <import>import poesweather</import>
  <make>poesweather.fy1_hrpt_decoder($verbose,$output)</make>

  <param>
    <name>Verbose</name>
    <key>verbose</key>
    <value>True</value>
    <type>bool</type>
  </param>

  <param>
    <name>Output Files</name>
    <key>output</key>
    <value>False</value>
    <type>bool</type>
  </param>

  <sink>
    <name>in</name>
    <type>short</type>
  </sink>

  <doc>
Feng-Yun 1 POES HRPT Decoder block.

This block connects to the output of the POES-Weather FY-1 HRPT deframer.
  </doc>
</block>
